{
  "name": "red_tetrisboilerplate",
  "version": "0.0.1",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run api-dev\" \"npm run client-dev\" \"npm run srv-dev\" --kill-others",
    "eslint": "eslint src",
    "client-dev": "NODE_ENV=development webpack-dev-server --colors --hot --inline --host 0.0.0.0 --port 8080",
    "client-dist": "NODE_ENV=production webpack --progress --colors",
    "srv-dev": "NODE_ENV=development nodemon --watch src/server --exec babel-node src/server/main.js",
    "srv-dist": "NODE_ENV=production babel src --out-dir dist",
    "api-dev": "NODE_ENV=development nodemon --watch src/api --exec babel-node src/api/main.js",
    "api-dist": "NODE_ENV=production babel src --out-dir dist",
    "test": "NODE_ENV=test mocha --require babel-core/register --reporter spec",
    "coverage": "NODE_ENV=test nyc --include src --all -r text -r lcov -r text --require @babel/register mocha "
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "joi": "^14.3.1",
    "joi-objectid": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.9",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-media": "^1.9.2",
    "react-particle-effect-button": "^1.0.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-socket.io-middleware": "^1.0.4",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@svgr/webpack": "4.3.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.1",
    "config": "^3.2.2",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.14.3",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  }
}
